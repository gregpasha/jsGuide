			"getFileSize",
			"\"",
			"var adv = window.adv || {};\n// adv.format = {};\n\ndefine(function(require) {\n    \"use strict\";\n    \n    require(\"./adv.format\");\n    var utils = require(\"./adv.utils\");\n   \n    adv.format.types = { Currency: 'Currency', Percentage: 'Percentage', Float: 'Float', Int: 'Int'};\n    adv.format.toCurrency = function (val) {\n        if (val === null) { return \"\"; }\n        var dollars, cents;\n        var value = adv.format.toString(val, adv.format.types.Currency); //todo remove kendo dependancy from formatting\n        // If negative remove round parenthesis before superscript\n        if (parseInt(value, 10) < 0) {\n            value = value.substring(1, value.length); //remove -\n            dollars = value.substring(0, value.length - 2);\n            cents = value.slice(-2);\n            return \"($\" + dollars + \"<sup>\" + cents + \"</sup>)\";\n        } else {\n            dollars = value.substring(0, value.length - 2);\n            cents = value.slice(-2);    \n            return \"$\" + dollars + \"<sup>\" + cents + \"</sup>\";\n        }\n    };\n\n    adv.format.toString = function (num, type, decimalCount) {\n\n        var p, negative = '';\n        \n        if (typeof num === \"string\") {\n            num = parseFloat(num);\n        }\n        decimalCount = decimalCount || 2;\n        if (!utils.isNumeric(num)) {\n            //throw num + ' is not a supported number';\n            utils.logger('Error -> '+num + ' is not a supported number');\n            return \"\";\n        }\n        switch (type) {\n            case adv.format.types.Currency:     \n                if (num < 0) {\n                    num = Math.abs(num);\n                    p = num.toFixed(decimalCount).split(\".\");\n                    return \"-\" + p[0].split(\"\").reverse().reduce(function (acc, num, i, orig) {\n                        return num + (i && !(i % 3) ? \",\" : \"\") + acc;\n                    }, \"\") + \".\" + p[1] + \"\";\n                } else {\n                    p = num.toFixed(decimalCount).split(\".\");\n                    return \"\" + p[0].split(\"\").reverse().reduce(function (acc, num, i, orig) {\n                        return num + (i && !(i % 3) ? \",\" : \"\") + acc;\n                    }, \"\") + \".\" + p[1];\n                }\n            case adv.format.types.Percentage:\n                p = num.toFixed(decimalCount).split(\".\");\n                // don't factor negatives into the equation\n                if (p[0][0] === '-') {\n                    p[0] = p[0].substring(1);\n                    negative = '-';\n                } \n                return negative + p[0].split(\"\").reverse().reduce(function (acc, num, i, orig) {\n                    return num + (i && !(i % 3) ? \",\" : \"\") + acc;\n                }, \"\") + \".\" + p[1] +'<sup>%</sup>';\n            case adv.format.types.Int:            \n                if (num < 0) {\n                    num = Math.abs(num);\n                    p = num.toFixed(0).split(\".\");\n                    return \"-\" + p[0].split(\"\").reverse().reduce(function (acc, num, i, orig) {\n                        return num + (i && !(i % 3) ? \",\" : \"\") + acc;\n                    }, \"\");\n\n                } else {\n                    p = num.toFixed(0).split(\".\");\n                    return p[0].split(\"\").reverse().reduce(function (acc, num, i, orig) {\n                        return num + (i && !(i % 3) ? \",\" : \"\") + acc;\n                    }, \"\");\n                }        \n            case adv.format.types.Float:\n                if (num < 0) {\n                    num = Math.abs(num);\n                    p = num.toFixed(decimalCount).split(\".\");\n                    return \"-\"+p[0].split(\"\").reverse().reduce(function (acc, num, i, orig) {\n                        return num + (i && !(i % 3) ? \",\" : \"\") + acc;\n                    }, \"\") + \".\" + p[1];\n\n                } else {\n                    p = num.toFixed(decimalCount).split(\".\");\n                    return p[0].split(\"\").reverse().reduce(function (acc, num, i, orig) {\n                        return num + (i && !(i % 3) ? \",\" : \"\") + acc;\n                    }, \"\") + \".\" + p[1];\n                }\n        }\n        \n    };\n\n    adv.format.positiveNegativeCurrency = function (value, currencyString) {    \n        if (value < 0) {\n            return \"<span class='adv-warning-color'>\" + currencyString + \"</span>\";\n        }\n        else if (value > 0) {\n            return \"<span class='adv-positive-color'>\" + currencyString + \"</span>\";\n        } else {\n            return \"<span>\" + currencyString + \"</span>\";\n        }\n    };\n\n    adv.format.positiveNegativePercentage = function (value) {\n\n        var amountString = adv.format.toString(value, adv.format.types.Percentage, 2);\n\n        if (value < 0) {\n            return \"<span class='adv-warning-color'>\" + amountString + \"</span>\";\n        }\n        else if (value > 0) {\n            return \"<span class='adv-positive-color'>\" + amountString + \"</span>\";\n        } else {\n            return \"<span>\" + amountString + \"</span>\";\n        }\n    };\n\n    //adv.format.makeDonuts = function (val) {\n    //    var perValue = val / 100;\n    //    var pctAssets = adv.format.toString(perValue, \"p1\", true);\n    //    var pctAngle = perValue * 360;\n\n    //    if (pctAssets != \"\") {\n    //        if (Math.abs(perValue) < 0.50) {\n    //            return \"<div class='adv-mini-donut-container'><div class='adv-mini-donut-background'></div><div class='adv-mini-donut-slice adv-hold'><div class='adv-mini-donut' data-pctangle='\" + pctAngle + \"' ></div></div></div><div class='adv-mini-donut-hole'></div><div class='adv-mini-donut-legend'>\" + pctAssets + \"</div>\";\n    //        }\n    //        else {\n    //            //slices that take up more that 50% of chart need to be generated as a 180 degree slice and then a second slice with the remainder\n    //            return \"<div class='adv-mini-donut-container'><div class='adv-mini-donut-background'></div><div class='adv-mini-donut-slice adv-hold'><div class='adv-mini-donut' style='-webkit-transform:rotate(180deg);'></div></div><div class='adv-mini-donut-slice adv-hold'style='-webkit-transform:rotate(180deg);'><div class='adv-mini-donut' style='-webkit-transform:rotate(\" + (pctAngle - 180) + \"deg);'></div></div></div><div class='adv-mini-donut-hole'></div><div class='adv-mini-donut-legend'>\" + pctAssets + \"</div>\";\n    //        }\n    //    }\n    //    return \"\";\n        \n    //};\n    //method with no kendo dependancy\n    adv.format.makeChart = function (val) {\n        var perValue = val / 100;\n        var pctAssets = adv.format.toString(val, adv.format.types.Percentage); //ADV.toString(val, \"p1\", true); \n        var pctAngle = perValue * 360;\n\n        if (pctAssets != \"\") {\n            if (Math.abs(perValue) < 0.50) {\n                return \"<div class='adv-mini-donut-container'><div class='adv-mini-donut-background'></div><div class='adv-mini-donut-slice adv-hold'><div class='adv-mini-donut' data-pctangle='\" + pctAngle + \"' ></div></div></div><div class='adv-mini-donut-hole'></div><div class='adv-mini-donut-legend'>\" + pctAssets + \"</div>\";\n            }\n            else {\n                //slices that take up more that 50% of chart need to be generated as a 180 degree slice and then a second slice with the remainder\n                return \"<div class='adv-mini-donut-container'><div class='adv-mini-donut-background'></div><div class='adv-mini-donut-slice adv-hold'><div class='adv-mini-donut' style='-webkit-transform:rotate(180deg);'></div></div><div class='adv-mini-donut-slice adv-hold'style='-webkit-transform:rotate(180deg);'><div class='adv-mini-donut' style='-webkit-transform:rotate(\" + (pctAngle - 180) + \"deg);'></div></div></div><div class='adv-mini-donut-hole'></div><div class='adv-mini-donut-legend'>\" + pctAssets + \"</div>\";\n            }\n        }\n        return \"\";\n\n    };\n    adv.format.toDate = function () {\n        var token = /d{1,4}|m{1,4}|yy(?:yy)?|([HhMsTt])\\1?|[LloSZ]|\"[^\"]*\"|'[^']*'/g,\n              timezone = /\\b(?:[PMCEA][SDP]T|(?:Pacific|Mountain|Central|Eastern|Atlantic) (?:Standard|Daylight|Prevailing) Time|(?:GMT|UTC)(?:[-+]\\d{4})?)\\b/g,\n              timezoneClip = /[^-+\\dA-Z]/g,\n              pad = function (val, len) {\n                  val = String(val);\n                  len = len || 2;\n                  while (val.length < len) val = \"0\" + val;\n                  return val;\n              };\n\n        // Regexes and supporting functions are cached through closure\n        return function (date, mask, utc) {\n            var dF = adv.format.toDate;\n\n            // You can't provide utc if you skip other args (use the \"UTC:\" mask prefix)\n            if (arguments.length === 1 && Object.prototype.toString.call(date) === \"[object String]\" && !/\\d/.test(date)) {\n                mask = date;\n                date = undefined;\n            }\n\n            // Passing date through Date applies Date.parse, if necessary\n            date = date ? new Date(date) : new Date();\n            if (isNaN(date)) throw SyntaxError(\"invalid date\");\n\n            mask = String(dF.masks[mask] || mask || dF.masks[\"default\"]);\n\n            // Allow setting the utc argument via the mask\n            if (mask.slice(0, 4) === \"UTC:\") {\n                mask = mask.slice(4);\n                utc = true;\n            }\n\n            var _ = utc ? \"getUTC\" : \"get\",\n                     d = date[_ + \"Date\"](),\n                     D = date[_ + \"Day\"](),\n                     m = date[_ + \"Month\"](),\n                     y = date[_ + \"FullYear\"](),\n                     H = date[_ + \"Hours\"](),\n                     M = date[_ + \"Minutes\"](),\n                     s = date[_ + \"Seconds\"](),\n                     L = date[_ + \"Milliseconds\"](),\n                     o = utc ? 0 : date.getTimezoneOffset(),\n                     flags = {\n                         d: d,\n                         dd: pad(d),\n                         ddd: dF.i18n.dayNames[D],\n                         dddd: dF.i18n.dayNames[D + 7],\n                         m: m + 1,\n                         mm: pad(m + 1),\n                         mmm: dF.i18n.monthNames[m],\n                         mmmm: dF.i18n.monthNames[m + 12],\n                         yy: String(y).slice(2),\n                         yyyy: y,\n                         h: H % 12 || 12,\n                         hh: pad(H % 12 || 12),\n                         H: H,\n                         HH: pad(H),\n                         M: M,\n                         MM: pad(M),\n                         s: s,\n                         ss: pad(s),\n                         l: pad(L, 3),\n                         L: pad(L > 99 ? Math.round(L / 10) : L),\n                         t: H < 12 ? \"a\" : \"p\",\n                         tt: H < 12 ? \"am\" : \"pm\",\n                         T: H < 12 ? \"A\" : \"P\",\n                         TT: H < 12 ? \"AM\" : \"PM\",\n                         Z: utc ? \"UTC\" : (String(date).match(timezone) || [\"\"]).pop().replace(timezoneClip, \"\"),\n                         o: (o > 0 ? \"-\" : \"+\") + pad(Math.floor(Math.abs(o) / 60) * 100 + Math.abs(o) % 60, 4),\n                         S: [\"th\", \"st\", \"nd\", \"rd\"][d % 10 > 3 ? 0 : (d % 100 - d % 10 !== 10) * d % 10]\n                     };\n\n            return mask.replace(token, function ($0) {\n                return $0 in flags ? flags[$0] : $0.slice(1, $0.length - 1);\n            });\n        };\n    } ();\n\n    // Some common format strings\n    adv.format.toDate.masks = {\n        \"default\": \"ddd mmm dd yyyy HH:MM:ss\",\n        shortDate: \"m/d/yy\",\n        mediumDate: \"mmm d, yyyy\",\n        longDate: \"mmmm d, yyyy\",\n        fullDate: \"dddd, mmmm d, yyyy\",\n        shortTime: \"h:MM TT\",\n        mediumTime: \"h:MM:ss TT\",\n        longTime: \"h:MM:ss TT Z\",\n        isoDate: \"yyyy-mm-dd\",\n        isoTime: \"HH:MM:ss\",\n        isoDateTime: \"yyyy-mm-dd'T'HH:MM:ss\",\n        isoUtcDateTime: \"UTC:yyyy-mm-dd'T'HH:MM:ss'Z'\"\n    };\n\n    // Internationalization strings\n    adv.format.toDate.i18n = {\n        dayNames: [\n              \"Sun\", \"Mon\", \"Tue\", \"Wed\", \"Thu\", \"Fri\", \"Sat\",\n              \"Sunday\", \"Monday\", \"Tuesday\", \"Wednesday\", \"Thursday\", \"Friday\", \"Saturday\"\n        ],\n        monthNames: [\n              \"Jan\", \"Feb\", \"Mar\", \"Apr\", \"May\", \"Jun\", \"Jul\", \"Aug\", \"Sep\", \"Oct\", \"Nov\", \"Dec\",\n              \"January\", \"February\", \"March\", \"April\", \"May\", \"June\", \"July\", \"August\", \"September\", \"October\", \"November\", \"December\"\n        ]\n    };\n\n    return adv.formatter_old;\n});",
